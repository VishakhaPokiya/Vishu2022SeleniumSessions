<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Regression Test" thread-count="3" parallel="tests" verbose="5">


		<test name="Google Test">
		<parameter name="url" value="https://www.google.com"/>
		<parameter name="browser" value="chrome"/>
			<classes>
				<class name="AppTests.Selenium_21_GoogleTest" />
			</classes>
			
		</test>
		
		<test name="OrangeHRMTest Test">
		<parameter name="url" value="https://www.orangehrm.com/orangehrm-30-day-trial/?"/>
		<parameter name="browser" value="chrome"/>
		
			<classes>
				<class name="AppTests.Selenium_21_OrangeHrmTest" />
			</classes>
			
		</test>
		
		<test name="OpenCart Test">
		<parameter name="url" value="https://demo.opencart.com/index.php?route=account/login"/>
		<parameter name="browser" value="chrome"/>
			<classes>
				<class name="AppTests.Selenium_21_OpenCartTest" />
			</classes>
			
		</test>
		
</suite>